{
  "name": "u-unpack",
  "version": "1.0.0",
  "description": "A command-line tool for unpacking Unity Package (.unitypackage) files",
  "main": "dist/index.js",
  "bin": {
    "u-unpack": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf test/integration-test-* src/integration-test-*",
    "all": "npm run lint && npm run build && npm run test && npm run clean"
  },
  "keywords": [
    "unity",
    "unitypackage",
    "unpack",
    "extract",
    "cli"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/tar": "^6.1.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "tar": "^6.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
