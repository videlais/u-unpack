{
  "name": "u-unpack",
  "version": "1.0.0",
  "description": "A command-line tool for unpacking Unity Package (.unitypackage) files",
  "main": "dist/index.js",
  "bin": {
    "u-unpack": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf test/integration-test-* src/integration-test-*",
    "all": "npm run lint && npm run build && npm run test && npm run clean"
  },
  "keywords": [
    "unity",
    "unitypackage",
    "unpack",
    "extract",
    "cli"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "jest": "^30.2.0",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "tar": "^7.5.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
